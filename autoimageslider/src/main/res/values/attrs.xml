        <attr name="ais_corner_radius" format="integer" />
        <attr name="ais_time_interval" format="integer" />
        <attr name="ais_slide_delay" format="integer" />
        <attr name="ais_placeholder" format="reference" />
        <attr name="ais_exception_image" format="reference" />
